<button class="btn btn-success" onclick="ExportToExcel('xlsx')"><i class="far fa-file-excel fs-3"></i> Exporter au fotmat Excel</button>



{{ form_start(form)}}
{{ form_widget(form)}}
<button class="btn btn-info">Ajouter un emmargement</button>
{{ form_end(form)}}


<script type="text/javascript" src="https://unpkg.com/xlsx@0.15.1/dist/xlsx.full.min.js"></script>
<script>
function ExportToExcel(type, fn, dl) {
       var elt = document.getElementById('tbl_exporttable_to_xls');
       var wb = XLSX.utils.table_to_book(elt, { sheet: "sheet1" });
       return dl ?
         XLSX.write(wb, { bookType: type, bookSST: true, type: 'base64' }):
         XLSX.writeFile(wb, fn || ("{{user.username}}" +'.' + (type || 'xlsx')));
    }
</script>